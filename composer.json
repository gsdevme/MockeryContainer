{
    "name": "gsdev/mockery-container",
    "description": "A mock container that works with the mocking library Mockery",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MockeryContainer\\": [
                "src/MockeryContainer"
            ]
        }
    },
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "mockery/mockery": "1.0.x-dev",
        "doctrine/collections": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "symfony/dependency-injection": "^2.8",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.6.4"
    }
}
