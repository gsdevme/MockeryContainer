{
    "name": "gsdev/mockery-container",
    "description": "A mock container that works with the mocking library Mockery",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MockeryContainer\\": [
                "src/MockeryContainer"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MockeryContainer\\": [
                "tests/MockeryContainer"
            ]
        }
    },
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "mockery/mockery": ">=0.9.2",
        "doctrine/collections": "~1.0"
    },
    "suggests": {
        "symfony/dependency-injection": "If you want to use the SymfonyMockeryContainer"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "symfony/dependency-injection": ">=2.6|>=3.0",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.6.4",
        "nikic/php-parser": "~3.0",
        "squizlabs/php_codesniffer": "3.0.0RC4",
        "satooshi/php-coveralls": "~0.6"
    }
}
